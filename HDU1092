import java.util.Scanner;

public class Main
{
	public static void main(String[] args)
	{
	Scanner s= new Scanner(System.in);
     int n=0;
     int sum=0;
      while (s.hasNextInt())
      {
	   sum=0;
	   n=s.nextInt();
	   if (n==0)
	   {
		break;   
	   }
	   else
	   {
		   int []a=new int [n];
		   for (int i=0;i<n;i++)
		   {
			   a[i]=s.nextInt();
			   sum=sum+a[i];
		   }
	   }
	 System.out.println(sum);
      }
	}
}
