import java.util.Scanner;

public class  HDU1021
{
	public static void main(String[] args)
	{
		Scanner cin=new Scanner(System.in); 
		while (cin.hasNextInt())
		{ 
			int n=cin.nextInt();
			n=n+2;
			if ( n%4==0)
				System.out.println("yes");
			else  
				System.out.println("no");
		}
	}
}


