11515070_AC_265ms_9908kB
import java.util.Arrays;
import java.util.Scanner;

public class HDU1004
{

	public static void main(String[] args) 
	{
		Scanner in =new Scanner(System.in);
		int a;
		String e;
		while(in.hasNextInt())
		{
			a=in.nextInt();
			if(a==0||a>1000)break;
			String[] c=new String[a];
			int[] d =new int[a];
			Arrays.fill(d, 0);
			for(int i=0;i<a;i++)
			{
				e=in.next();
				c[i]=e;
				for(int j=0;j<=i;j++)
				{
					if(e.equals(c[j]))
					{
						d[j]++;
						break;
					}
				}
				
			}
			System.out.println(c[max(d)]);
			
		}

	}
	
    public static int max(int[] a)
    {
    	int n=a.length;
    	int max=a[0];
    	int count=0;
    	for(int i=0;i<n;i++)
    	{
    		if(max<a[i])
    		{
    			max=a[i];
    			count=i;
    		}
    	}
    	return count;
    }
}


