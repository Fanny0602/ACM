import java.util.Scanner;

public class HDU1001
{
	public static void main(String[] args)
	{
      Scanner s=new Scanner(System.in);
       int  sum=0;
      while (s.hasNextInt())
      {
    	  int n=s.nextInt();
        for (int i=1;i<=n;i++)
        {
        	sum=sum+i;
        }
        System.out.println(sum);
        System.out.println();
        sum=0;
      }
	}
}
