
import java.util.Scanner;

public class Main
{

	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
		int a=0;
		int b=0;
		while(s.hasNextInt())
		{
			a=s.nextInt();
			b=s.nextInt();
			if(a==0 && b==0)
			{
				break;
			}
			else
			{
				System.out.println(a+b);
			}
		}
	}
}
